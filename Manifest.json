{
    "manifest_version": 3,
    "name": "AdvHistory",
    "version": "1.0",
    "description": "Search your browser history by date, day, time range, or topic.",
    "permissions": ["history", "tabs"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "prashant.png",
        "48": "prashant.png",
        "128": "prashant.png"
      }
    },
    "icons": {
      "16": "prashant.png",
      "48": "prashant.png",
      "128": "prashant.png"
    }
  }
  